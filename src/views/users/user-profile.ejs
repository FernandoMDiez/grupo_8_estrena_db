<!DOCTYPE html>
<html lang="en">

<!-- se obtiene el head de la carpeta Partials -->
<%- include("../partials/head-users") %>


  <body>
    <!-- se obtiene el header de la carpeta  Partials -->
    <%- include("../partials/header") %>


      <main>
        <form method="POST" action="/users/user-profile">
          <div class="register-container">

            <h1 class="title">Bienvenido</h1>
            <div class="inputs">
              <p>Nombre</p>
              <p> Usuario: <%= userL.firstName %>
              </p>
              <img src="/images/users/<%= userL.image %>" alt="">

              <div class="account-buttons">
                <a class="account res" href="/">Cancelar</a>
                <button type="submit" class="account sub">Cerrar Session</button>
              </div>

            </div>

          </div>
        </form>
      </main>
      <!-- se obtiene el footer de la carpeta Partials -->
      <%- include("../partials/footer") %>
  </body>

</html>